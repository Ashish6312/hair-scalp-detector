{
  "version": 2,
  "builds": [
    {
      "src": "minor/minor/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/minor/myapp/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "minor/minor/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "minor.settings_production"
  }
}
